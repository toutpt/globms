<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="collective.googledocsviewer">
<body>
<div metal:fill-slot="main" class="container_12">
  <h1 id="pagetitle" class="grid_12 alpha omega hiddenStructure">Global Market Solution: Financial Software Technology</h1>
  <div class="clear"></div>
  <tal:block repeat="block python:here.getMyBlocks('home_center')">
  <tal:def define="extra_class string:grid_3 homecenterb; index repeat/block/index;
                   extra_class python:repeat['block'].start and extra_class + ' alpha' or extra_class;
                   extra_class python:repeat['block'].end and extra_class + ' omega' or extra_class;">
    <div tal:attributes="class extra_class;id string:homecenterb${repeat/block/index}">
        <h2 tal:content="block/Title">block title</h2>
        <img tal:define="scales block/@@images;
                  thumb python: scales.scale('image', width=50, height=50);"
                  tal:condition="thumb"
                  tal:attributes="src thumb/url;
                                  width thumb/width;
                                  height thumb/height" />
        <p tal:replace="structure block/getText"></p>
    </div>
  </tal:def>
  </tal:block>


  <div class="clear"></div>
<div id="blockhomebas-wrapper" class="container_12">
  <div id="blockhomebas" class="grid_12 alpha omega">
  <tal:block repeat="block python:here.blocks.getMyBlocks('home_last')">
  <tal:def define="extra_class string:grid_4 blockhomebas; index repeat/block/index;
                   extra_class python:repeat['block'].start and extra_class + ' alpha' or extra_class;
                   extra_class python:repeat['block'].end and extra_class + ' omega' or extra_class;">
    <div tal:attributes="class extra_class; id string:blockhomebas$index">
        <img tal:define="scales block/@@images;
                  thumb python: scales.scale('image', width=84, height=84);"
                  tal:condition="thumb"
                  tal:attributes="src thumb/url;
                                  width thumb/width;
                                  height thumb/height" />
        <h2 tal:content="block/Title">block title</h2>
        <p tal:replace="structure block/getText"></p>
    </div>
  </tal:def>
  </tal:block>
  </div>
</div>
  <div class="clear"></div>
<div class="container_12">


<!-- root element for scrollable -->
<div class="scrollable">

   <!-- root element for the items -->
   <div class="items">

  <div>
  <tal:block repeat="block python:here.getMyBlocks('home_partners')">
  <tal:def define="extra_class string:grid_3 partenaire; index repeat/block/index;
                   extra_class python:repeat['block'].start and extra_class + ' alpha' or extra_class;
                   extra_class python:repeat['block'].end and extra_class + ' omega' or extra_class;">
     <div tal:attributes="class extra_class">
         <p tal:replace="structure block/getText"></p>
     </div>
  </tal:def>
  </tal:block>
  </div>
  </div>
</div>

</div>

<script type="text/javascript">
$(".slidetabs").tabs(".images > div", {effect: 'fade',fadeOutSpeed: "fast",rotate: true}).slideshow();
$(".scrollable").scrollable({circular:true}).autoscroll({interval:3000});
$(".blockhomebas").equalHeights();
</script>


</div>

</body>
</html>