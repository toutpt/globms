 <div id="slideshowwrapper">
  <div id="slideshow" class="grid_9 alpha">
    <tal:comment replace="nothing">THIS NEED A /slideshow folder with block inside</tal:comment>
    <div class="images" tal:repeat="slide python:here.getMyBlocks('home_slideshow')">
        <div><img tal:define="scales slide/@@images;
                  slidescale python: scales.scale('image', width=700, height=269);"
                  tal:condition="slidescale"
                  tal:attributes="src slidescale/url;
                                  width slidescale/width;
                                  height slidescale/height" /></div>
    </div>
    <div class="slidetabs">
      <tal:block tal:repeat="slide python:here.getMyBlocks('home_slideshow')">
         <a href="#"></a>
      </tal:block>
    </div>
  </div>
  <div id="rightblock" class="grid_3 omega" tal:define="rightblock python:here.blocks.home_rightblock">
    <h2 tal:content="rightblock/Title">block title</h2>
    <p tal:replace="structure rightblock/getText">block content</p>
  </div>

  <div class="clear"></div>
 </div>