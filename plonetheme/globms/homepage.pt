<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plonetheme.globms">
<body>
<div metal:fill-slot="main" class="container_12"
     tal:define="is_anonymous view/@@plone_portal_state/anonymous;">
  <h1 id="pagetitle" class="grid_12 alpha omega hiddenStructure" tal:content="view/site_title"></h1>
  <div class="clear"></div>
  <div class="grid_10 alpha">
            <div id="viewlet-below-content-body" tal:content="structure provider:plone.abovecontentbody" />
            <div id="viewlet-below-content-body" tal:content="structure provider:plone.belowcontentbody" />
  </div>
  <div class="grid_2 omega">
    <div class="contact">
      <h3><a tal:attributes="href string:${context/absolute_url}/contact-us">Contact us</a></h3>
    </div>
    <div class="support"
         tal:define="member context/@@plone_portal_state/member;
                    member_id member/getId|nothing;
                    has_support python:path('here/intranet/%s | nothing' % member_id)">
      <h3><a tal:attributes="href string:${context/absolute_url}/intranet" tal:condition="is_anonymous">Support</a>
          <a tal:attributes="href string:${context/absolute_url}/intranet/${member_id}" tal:condition="python: not(is_anonymous) and has_support">Support</a>
          <a tal:attributes="href string:${context/absolute_url}/support" tal:condition="python: not(is_anonymous) and not(has_support)">Support</a>
      </h3>
    </div>
  </div>
  <script type="text/javascript">
  $(".slidetabs").tabs(".images > div", {effect: 'fade',fadeOutSpeed: "fast",rotate: true}).slideshow();
  $(".scrollable").scrollable({circular:true,speed:1000}).autoscroll({interval:6000});
  </script>
</div>

</body>
</html>
